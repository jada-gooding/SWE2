<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.a32gbfoodorderapp">

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name=".splashScreen">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name=".BillActivity" />
        <activity android:name=".drinksDetail" />
        <activity android:name=".drinksList" />
        <activity android:name=".dessertDetail" />
        <activity android:name=".dessertList" />
        <activity android:name=".mainCourseDetail" />
        <activity android:name=".mainCourseList" />
        <activity android:name=".AppetizerDetail" />
        <activity android:name=".AppetizerList" />
        <activity android:name=".MainActivity"/>
    </application>

</manifest>